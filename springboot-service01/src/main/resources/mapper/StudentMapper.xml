<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.project.dao.StudentMapper">
    <select id="getListStudent" resultType="com.springboot.project.entity.Student">
        select * from student
    </select>
    <select id="getId" parameterType="Integer" resultType="com.springboot.project.entity.Student">
        select * from student where id=#{id}
    </select>

</mapper>
