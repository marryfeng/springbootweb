<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.project.dao.StudentMapper">
    <select id="getStudentByID" resultType="com.springboot.project.entity.Student" parameterType="Integer">
        select * from student where id=#{id}
    </select>


